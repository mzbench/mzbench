{application, mzbench, [
  {description, ""},
  {vsn, "1.0.0"},
  {registered, []},
  {applications, [
    kernel,
    stdlib,
    crypto,
    lager,
    jiffy,
    luerl,
    sasl,
    ranch,
    mz_histogram,
    mzbench_language,
    mzbench_utils,
    os_mon,
    mz_counter
  ]},
  {mod, { mzb_app, []}},
  {env, [
    {node_deployment_path, "~/.local/share"},
    {workers_dirs, ["~/.local/share/mzbench_workers", "../workers", "./workers"]},
    {node_log_port, 4801},
    {node_log_user_port, 4802},
    {node_management_port, 4803},
    {node_interconnect_port, 4804},
    {gc_sleep, 10},
    {log_queue_max_len, 1000},
    {log_rate_limit, 200},
    {metric_update_interval_ms, 10000},
    {time_sync_interval_s, 60}
  ]}
]}.
